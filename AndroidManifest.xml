    package="com.example.jesus.veterinaria">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Principal" />
        <activity android:name=".Clientes" />
        <activity
            android:name=".ListaClientes"
            android:label="@string/title_activity_lista_clientes"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Reistro_Cliente" />
        <activity android:name=".registro_mascota" />
        <activity android:name=".registro_historial" />
        <activity android:name=".registro_medico" />
        <activity android:name=".Medicos" />
        <activity android:name=".Mascotas" />
        <activity android:name=".Servicios"></activity>
    </application>

</manifest>